{
  "name": "js-var-type",
  "version": "0.2.5",
  "description": "A JS library to check variable type at runtime",
  "main": "dist/main.js",
  "type": "module",
  "exports": {
    "import": "./dist/main.js",
    "require": "./dist/main.cjs"
  },
  "scripts": {
    "build": "rollup --config",
    "release": "standard-version",
    "prepublishOnly": "npm run build",
    "eslint": "eslint .",
    "prettier": "prettier .",
    "ci": "npm run prettier -- --list-different && npm run eslint",
    "lint": "npm run eslint -- --fix",
    "format": "npm run prettier -- --write",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/assisrafael/js-var-type.git"
  },
  "keywords": [
    "types",
    "runtime"
  ],
  "author": "Igor Rafael <igor.rafael@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/assisrafael/js-var-type/issues"
  },
  "homepage": "https://github.com/assisrafael/js-var-type#readme",
  "devDependencies": {
    "@babel/core": "7.12.9",
    "@babel/plugin-transform-runtime": "7.12.1",
    "@babel/preset-env": "7.12.7",
    "@babel/runtime": "7.12.5",
    "@rollup/plugin-babel": "5.2.2",
    "@types/jest": "26.0.16",
    "babel-jest": "26.6.3",
    "eslint": "7.14.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "rollup": "2.34.1",
    "standard-version": "9.0.0"
  }
}
