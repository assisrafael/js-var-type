{
  "name": "js-var-type",
  "version": "0.2.3",
  "description": "A JS library to check variable type at runtime",
  "main": "dist/main.js",
  "type": "module",
  "exports": {
    "import": "./dist/main.js",
    "require": "./dist/main.cjs"
  },
  "scripts": {
    "build": "rollup --config",
    "release": "standard-version",
    "prepublishOnly": "npm run build",
    "eslint": "eslint {src,test}\"/**/*.\"{js,jsx}",
    "prettier": "prettier {src,test}\"/**/*.\"{js,jsx}",
    "ci": "npm run prettier -- --list-different && npm run eslint",
    "lint": "npm run eslint -- --fix",
    "format": "npm run prettier -- --write",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/assisrafael/js-var-type.git"
  },
  "keywords": [
    "types",
    "runtime"
  ],
  "author": "Igor Rafael <igor.rafael@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/assisrafael/js-var-type/issues"
  },
  "homepage": "https://github.com/assisrafael/js-var-type#readme",
  "devDependencies": {
    "@babel/core": "7.11.4",
    "@babel/preset-env": "7.11.0",
    "@rollup/plugin-node-resolve": "9.0.0",
    "@types/jest": "26.0.10",
    "babel-jest": "26.3.0",
    "eslint": "7.7.0",
    "jest": "26.4.2",
    "prettier": "2.1.1",
    "rollup": "2.26.5",
    "rollup-plugin-babel": "4.4.0",
    "standard-version": "^9.0.0"
  }
}
